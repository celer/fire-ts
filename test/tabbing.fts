# No tabs
<% 
if(true){
	%>
	Hello
	<%
	var colors=["red","green","blue"];
	var numbers=[1,2,3];
	for(var i in colors){ 
		var color=colors[i]; %>
		<%= color%>
		Color <%= color%>
		<% for(var j in numbers){ %>
			<%= numbers[j] %>
			Number <%= numbers[j] %>
			Number <%# numbers[j] %>
			Number & Color <%=color+" "+numbers[j]%>
		<%}%>
	<%	}
}
%>



